
# Frontend Configuration
VITE_APP_NAME=Hokela API LogFlow Dashboard

# Backend API Configuration
VITE_API_URL=https://hokela-logger.onrender.com
VITE_WS_URL=wss://hokela-logger.onrender.com

# Authentication (if needed)
VITE_JWT_SECRET_KEY=your-frontend-jwt-secret

# External Services (if needed)
VITE_ANALYTICS_KEY=your-analytics-key
VITE_MONITORING_KEY=your-monitoring-service-key

# Development/Production Environment
VITE_NODE_ENV=production

# Optional: Feature flags
VITE_ENABLE_REALTIME=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EXPORT=true

# Optional: UI Configuration
VITE_THEME=dark
VITE_REFRESH_INTERVAL=5000
